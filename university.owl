<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.org/university#"
     xml:base="http://www.example.org/university"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://www.example.org/university"/>
    
    <!-- Classes -->
    <owl:Class rdf:about="http://www.example.org/university#Person"/>
    
    <owl:Class rdf:about="http://www.example.org/university#Student">
        <rdfs:subClassOf rdf:resource="http://www.example.org/university#Person"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/university#Faculty">
        <rdfs:subClassOf rdf:resource="http://www.example.org/university#Person"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/university#Course"/>
    
    <owl:Class rdf:about="http://www.example.org/university#Department"/>
    
    <owl:Class rdf:about="http://www.example.org/university#UndergraduateStudent">
        <rdfs:subClassOf rdf:resource="http://www.example.org/university#Student"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/university#GraduateStudent">
        <rdfs:subClassOf rdf:resource="http://www.example.org/university#Student"/>
    </owl:Class>
    
    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://www.example.org/university#teaches">
        <rdfs:domain rdf:resource="http://www.example.org/university#Faculty"/>
        <rdfs:range rdf:resource="http://www.example.org/university#Course"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.example.org/university#enrolledIn">
        <rdfs:domain rdf:resource="http://www.example.org/university#Student"/>
        <rdfs:range rdf:resource="http://www.example.org/university#Course"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.example.org/university#belongsTo">
        <rdfs:domain rdf:resource="http://www.example.org/university#Person"/>
        <rdfs:range rdf:resource="http://www.example.org/university#Department"/>
    </owl:ObjectProperty>
</rdf:RDF>